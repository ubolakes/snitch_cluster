// Parameters for a GEMM

{
    gemmInfo: {
        prec: 64,
        M: 32,
        N: 32,
        K: 32,
        ta: false, // false = row-major, true = col-major
        tb: false,
        tc: false, // not implemented
    }

    gemmArgs: { // C = alpha * A * B + beta * C
        beta: 1,  // only beta={0,1} supported
        alpha: 1, // not implemented
    }

    gemmImpl: {
        method: "streambuffer",
        use_c2c_tiles: true,
        reversing_loops: true,
        L1_M: 16,
        L1_N: 16,
        L1_K: 8,
        ta_tile: false,
        tb_tile: false,
        tc_tile: false, // not implemented
        expand: 0,
        fmadd_d_unroll: 8, // requires code modification
    }
    
    bench: {
        iters: 1,
    }
    
    datagen: {
        linspace: true,
    }
}
